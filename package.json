{
  "name": "musiciodemo-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "check": "nsp check && snyk test --dev",
    "prod": "NODE_ENV=production node_modules/babel-cli/bin/babel-node.js app.js",
    "devb": "NODE_ENV=devb node_modules/nodemon/bin/nodemon.js -- node_modules/babel-cli/bin/babel-node.js app.js",
    "dev": "NODE_ENV=dev concurrently \"node_modules/nodemon/bin/nodemon.js -- node_modules/babel-cli/bin/babel-node.js app.js\" \"cd .. && cd musicio-frontend && yarn start\"",
    "start": "cross-env node_modules/pm2/bin/pm2 start npm -- run prod",
    "poststart": "node node_modules/pm2/bin/pm2 logs",
    "win": "cross-env NODE_ENV=production node_modules/babel-cli/bin/babel-node.js app.js",
    "test": "NODE_ENV=test mocha --require babel-register --exit"
  },
  "dependencies": {
    "@phc/format": "^0.4.2",
    "arangojs": "^6.3.0",
    "argon2": "^0.19.1",
    "aws-sdk": "^2.264.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "boom": "^7.2.0",
    "connect-redis": "^3.3.3",
    "csurf": "^1.9.0",
    "dotenv": "^5.0.1",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "helmet": "^3.12.1",
    "hpp": "^0.2.2",
    "joi": "^13.4.0",
    "mobile-detect": "^1.4.2",
    "morgan": "~1.9.0",
    "multer": "^1.3.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "passport-local": "^1.0.0",
    "pm2": "^2.10.4",
    "redis": "^2.8.0",
    "winston": "^2.4.3"
  },
  "devDependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "concurrently": "^3.6.0",
    "cookie-parse": "^0.4.0",
    "cross-env": "^5.1.6",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.10.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "snyk": "^1.85.0",
    "supertest": "^3.1.0"
  }
}
